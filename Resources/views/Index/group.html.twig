{% extends 'KamranTagsBundle::layout.html.twig' %}


{% block body %}
<script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>

    <h3>Add Tag</h3>
    <form class="entity" action="{{ path('tags_group') }}" method="post" {{ form_enctype(form) }} >

        {{ form_widget(form._token) }}

        <div class="form-group">
        {{ form_widget(form) }}
        </div>


        <div class="form-group">
            <input type="submit" class="btn btn-primary" value="create" />
        </div>
    </form>

    <br/><br/>


{% endblock body %}
